
    #include<stdio.h>
int main(){
    int num;
    scanf("%d",&num);
    int r;
    int sum=0;
    int sum1=0;
    int r1;
    if(num<=9){
        printf("%d ",num);
        //return num;
        //break;
    }
    else{
        
    for(int i=0;i<=num;i++){
        r=num%10;
        num=num/10;
        sum=sum+r;

    }
        for(int i=0;i<=sum;i++){
            r1=sum%10;
            sum=sum/10;
            sum1+=r1;
        }
        //printf("%d",sum1); 
    }
    
    
    printf("%d",sum1);
    
    return 0;
}
