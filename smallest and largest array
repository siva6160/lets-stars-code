#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[n];
    int max=arr[0];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int min=arr[0];
    for(int i=0;i<n;i++){
       if(arr[i]>max){
            max=arr[i];
        }
    
    int min=arr[0];
    //for(int i=0;i<n;i++){
        if(arr[i]<min){//i=0,1<1,2<1,
            min=arr[i];
            
        }
        
    }
    printf("%d ",max);
    printf("%d ",min);
    return 0;
}
